{
  "kind": "collectionType",
  "collectionName": "card_templates",
  "info": {
    "singularName": "card-template",
    "pluralName": "card-templates",
    "displayName": "Card Template",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "templateId": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "category": {
      "type": "string",
      "required": true
    },
    "variants": {
      "type": "json",
      "required": true
    },
    "tags": {
      "type": "json",
      "required": true
    },
    "accessLevel": {
      "type": "enumeration",
      "enum": [
        "public",
        "premium"
      ]
    },
    "isActive": {
      "type": "boolean"
    },
    "cards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::card.card",
      "mappedBy": "template"
    }
  }
}
