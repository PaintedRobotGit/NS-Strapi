{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "Company",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "company"
    },
    "positions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::position.position",
      "mappedBy": "company"
    },
    "slug": {
      "type": "uid"
    },
    "cards": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::card.card",
      "mappedBy": "company"
    },
    "settings": {
      "type": "json"
    },
    "website": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "subscription": {
      "type": "json"
    },
    "inviteCode": {
      "type": "string"
    },
    "company_invite_links": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::company-invite-link.company-invite-link",
      "mappedBy": "company"
    }
  }
